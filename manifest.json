{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["https://*.twitch.tv/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
        "service_worker": "service-worker.js"
      }
  }
  